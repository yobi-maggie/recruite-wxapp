/**index.wxss**/
.container {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
}
/* pages/deliver/deliver.wxss */
.tab {
  display: flex;
  border-bottom: 1px solid #e8e8e8;
  font-size: 14px;
  z-index: 1000;
  width: 100%;
}

.tab-item {
  flex-grow: 1;
  height: 42.5px;
  line-height: 42.5px;
  text-align: center;
  color: #555;
}

.tab-item.selected {
  color: #00b38a;
   background-color: #f5f5f5; 
}

ec-canvas {
  width: 100%;
  height: 100%;
}
